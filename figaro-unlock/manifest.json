{
  "manifest_version": 2,
  "name": "Figaro Unlock",
  "description": "Enleve les bannieres anti bloqueur de publicite du Figaro / Le Point / Marie Claire / L'Equipe / L'Express",
  "version": "1.0.2",
  "icons": {
    "16": "data/images/icon-16.png",
    "48": "data/images/icon-48.png",
    "128": "data/images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.lefigaro.fr/*",
        "https://*.lefigaro.fr/*",
        "http://*.lepoint.fr/*",
        "https://*.lepoint.fr/*",
        "http://*.marieclaire.fr/*",
        "https://*.marieclaire.fr/*",
        "http://*.lequipe.fr/*",
        "https://*.lequipe.fr/*",
        "http://*.lexpress.fr/*",
        "https://*.lexpress.fr/*"
      ],
      "css": [
        "data/style/content.css"
      ],
      "js": [
        "data/script/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "http://*.lefigaro.fr/*",
    "https://*.lefigaro.fr/*",
    "http://*.lepoint.fr/*",
    "https://*.lepoint.fr/*",
    "http://*.marieclaire.fr/*",
    "https://*.marieclaire.fr/*",
    "http://*.lequipe.fr/*",
    "https://*.lequipe.fr/*",
    "http://*.lexpress.fr/*",
    "https://*.lexpress.fr/*"
  ]
}